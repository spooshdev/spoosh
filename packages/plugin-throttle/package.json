{
  "name": "@spoosh/plugin-throttle",
  "version": "0.1.0",
  "description": "Request throttling plugin for Spoosh - limits request frequency",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nxnom/spoosh.git",
    "directory": "packages/plugin-throttle"
  },
  "bugs": { "url": "https://github.com/nxnom/spoosh/issues" },
  "homepage": "https://spoosh.dev/docs/plugins/throttle",
  "publishConfig": { "access": "public" },
  "keywords": ["spoosh", "plugin", "throttle", "api-client", "rate-limit"],
  "files": ["dist"],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --max-warnings 0",
    "format": "prettier --write 'src/**/*.ts'",
    "prepublishOnly": "npm run build && npm run typecheck && npm run lint"
  },
  "peerDependencies": {
    "@spoosh/core": ">=0.1.0"
  },
  "devDependencies": {
    "@spoosh/core": "workspace:*",
    "@spoosh/test-utils": "workspace:*"
  }
}
